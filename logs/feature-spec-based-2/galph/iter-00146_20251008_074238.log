+    ('b', py_vals.get('k_frac'), py_vals.get('k_round'), Nb),
+    ('c', py_vals.get('l_frac'), py_vals.get('l_round'), Nc),
+)
+for axis, frac, rounded, N in axes:
+    if frac is None or rounded is None:
