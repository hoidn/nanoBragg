+def parse_trace(path: Path, prefix: str):
+    data = {}
+    if not path.exists():
+        return data
+    with path.open() as fh:
+        for line in fh:
+            if not line.startswith(prefix):
+                continue
+            parts = line.strip().split()
+            if len(parts) < 3:
