+                data['k_round'] = int(parts[3])
+                data['l_round'] = int(parts[4])
+            elif key in {'F_latt', 'F_latt_a', 'F_latt_b', 'F_latt_c'}:
+                data[key] = float(parts[2])
+    return data
+
+def sincg(x: float, N: int) -> float:
+    if abs(x) < 1e-12:
+        return float(N)
+    return math.sin(N * x) / math.sin(x)
+
+def fmt(val: float) -> str:
+    if val is None:
+        return 'nan'
+    return f"{val:.9f}"
+
