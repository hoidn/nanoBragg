elf.close_distance\n    72→    print(f\"TRACE_PY:close_distance_m={close_distance_m:.15g}\")\n    73→\n    74→    ratio = self.r_factor.item() if isinstance(self.r_factor, torch.Tensor) else self.r_factor\n    75→    print(f\"TRACE_PY:ratio={ratio:.15g}\")\n    76→\n    77→    distance_m = self.distance_corrected.item() if isinstance(self.distance_corrected, torch.Tensor) else self.distance_corrected\n    78→    print(f\"TRACE_PY:distance_m={distance_m:.15g}\")\n    79→\n    80→    # For CUSTOM convention with custom vectors, always compute term_* regardless of pivot\n    81→    # These show the intermediate calculation steps\n    82→    if self.config.custom_fdet_vector:\n    83→        fdet_vec = torch.tensor(self.config.custom_fdet_vector, dtype=torch.float64)\n    84→        sdet_vec = torch.tensor(self.config.custom_sdet_vector, dtype=torch.float64)\n    85→        odet_vec = torch.tensor(self.config.custom_odet_vector, dtype=torch.float64)\n    86→\n    87→        term_fast = -Fclose_m * fdet_vec\n    88→        term_slow = -Sclose_m * sdet_vec\n    89→        term_close = close_distance_m * odet_vec\n    90→\n    91→        print(f\"TRACE_PY:term_fast_before_rot={term_fast[0].item():.15g} {term_fast[1].item():.15g} {term_fast[2].item():.15g}\")\n    92→        print(f\"TRACE_PY:term_slow_before_rot={term_slow[0].item():.15g} {term_slow[1].item():.15g} {term_slow[2].item():.15g}\")\n    93→        print(f\"TRACE_PY:term_close_before_rot={term_close[0].item():.15g} {term_close[1].item():.15g} {term_close[2].item():.15g}\")\n    94→\n    95→    # pix0 vector\n    96→    pix0 = self.pix0_vector\n    97→    pix0_vals = [pix0[i].item() if isinstance(pix0, torch.Tensor) else pix0[i] for i in range(3)]\n    98→    print(f\"TRACE_PY:pix0_before_rotation={pix0_vals[0]:.15g} {pix0_vals[1]:.15g} {pix0_vals[2]:.15g}\")\n    99→    print(f\"TRACE_PY:pix0_after_rotz={pix0_vals[0]:.15g} {pix0_vals[1]:.15g} {pix0_vals[2]:.15g}\")\n   100→    print(f\"TRACE_PY:pix0_after_twotheta={pix0_vals[0]:.15g} {pix0_vals[1]:.15g} {pix0_vals[2]:.15g}\")\n   101→\n   102→    # Detector basis vectors (after rotations)\n   103→    fdet = self.fdet_vec\n   104→    sdet = self.sdet_vec\n   105→    odet = self.odet_vec\n   106→    fdet_vals = [fdet[i].item() if isinstance(fdet, torch.Tensor) else fdet[i] for i in range(3)]\n   107→    sdet_vals = [sdet[i].item() if isinstance(sdet, torch.Tensor) else sdet[i] for i in range(3)]\n   108→    odet_vals = [odet[i].item() if isinstance(odet, torch.Tensor) else odet[i] for i in range(3)]\n   109→\n   110→    print(f\"TRACE_PY:fdet_after_rotz={fdet_vals[0]:.15g} {fdet_vals[1]:.15g} {fdet_vals[2]:.15g}\")\n   111→    print(f\"TRACE_PY:sdet_after_rotz={sdet_vals[0]:.15g} {sdet_vals[1]:.15g} {sdet_vals[2]:.15g}\")\n   112→    print(f\"TRACE_PY:odet_after_rotz={odet_vals[0]:.15g} {odet_vals[1]:.15g} {odet_vals[2]:.15g}\")\n   113→\n   114→    # twotheta_axis\n   115→    if hasattr(self.config, 'twotheta_axis') and self.config.twotheta_axis is not None:\n   116→        twotheta_axis = self.config.twotheta_axis\n   117→    else:\n   118→        twotheta_axis = (0, 1, 0)\n   119→    print(f\"TRACE_PY:twotheta_axis={twotheta_axis[0]:.15g} {twotheta_axis[1]:.15g} {twotheta_axis[2]:.15g}\")\n   120→\n   121→    print(f\"TRACE_PY:fdet_after_twotheta={fdet_vals[0]:.15g} {fdet_vals[1]:.15g} {fdet_vals[2]:.15g}\")\n   122→    print(f\"TRACE_PY:sdet_after_twotheta={sdet_vals[0]:.15g} {sdet_vals[1]:.15g} {sdet_vals[2]:.15g}\")\n   123→    print(f\"TRACE_PY:odet_after_twotheta={odet_vals[0]:.15g} {odet_vals[1]:.15g} {odet_vals[2]:.15g}\")\n   124→\n   125→# Install the patched version\n   126→Detector.__init__ = traced_detector_init\n   127→\n   128→def main():\n   129→    \"\"\"Run trace for specific pixel matching C command.\"\"\"\n   130→\n   131→    # Paths\n   132→    repo_root = Path(__file__).parent.parent.parent.parent\n   133→    mat_file = repo_root / \"A.mat\"\n   134→    hkl_file = repo_root / \"scaled.hkl\"\n   135→\n   136→    # Parameters from C command\n   137→    wavelength_A = 0.976800\n   138→    oversample = 1\n   139→    pix0_vector_mm = [-216.336293, 215.205512, -230.200866]\n   140→    flux = 1e18\n   141→    exposure = 1.0\n   142→    beamsize = 1.0\n   143→    spindle_axis = [-1.0, 0.0, 0.0]\n   144→    Xbeam = 217.742295\n   145→    Ybeam = 213.907080\n   146→    distance = 231.274660\n   147→    pixel_size = 0.172\n   148→    detpixels = (2463, 2527)  # (spixels, fpixels)\n   149→    odet_vector = [-0.000088, 0.004914, -0.999988]\n   150→    sdet_vector = [-0.005998, -0.999970, -0.004913]\n   151→    fdet_vector = [0.999982, -0.005998, -0.000118]\n   152→    beam_vector = [0.00051387949, 0.0, -0.99999986]\n   153→    N_cells = (36, 47, 29)\n   154→    osc = 0.1\n   155→    phi = 0.0\n   156→    phisteps = 10\n   157→\n   158→    # Target pixel (slow, fast)\n   159→    target_pixel = (1039, 685)\n   160→\n   161→    # Load crystal cell from matrix\n   162→    a_star, b_star, c_star = read_mosflm_matrix(str(mat_file), wavelength_A)\n   163→    cell_params = reciprocal_to_real_cell(a_star, b_star, c_star)\n   164→\n   165→    # Load HKL data\n   166→    hkl_data = read_hkl_file(str(hkl_file), default_F=0.0)\n   167→\n   168→    # Calculate fluence from flux/exposure/beamsize\n   169→    # fluence = flux * exposure / (beamsize_m^2)\n   170→    beamsize_m = beamsize / 1000.0  # mm to m\n   171→    fluence = flux * exposure / (beamsize_m * beamsize_m)\n   172→\n   173→    # Create crystal config\n   174→    crystal_config = CrystalConfig(\n   175→        cell_a=cell_params[0],\n   176→        cell_b=cell_params[1],\n   177→        cell_c=cell_params[2],\n   178→        cell_alpha=cell_params[3],\n   179→        cell_beta=cell_params[4],\n   180→        cell_gamma=cell_params[5],\n   181→        N_cells=N_cells,\n   182→        default_F=0.0,\n   183→        phi_start_deg=phi,\n   184→        osc_range_deg=osc,\n   185→        phi_steps=phisteps,\n   186→        mosaic_spread_deg=0.0,\n   187→        mosaic_domains=1,\n   188→        spindle_axis=tuple(spindle_axis),\n   189→        # Phase G: Pass MOSFLM orientation\n   190→        mosflm_a_star=a_star,\n   191→        mosflm_b_star=b_star,\n   192→        mosflm_c_star=c_star\n   193→    )\n   194→\n   195→    # Create detector config with CUSTOM convention\n   196→    # Convert pix0_vector from mm to meters\n   197→    pix0_vector_m = [x / 1000.0 for x in pix0_vector_mm]\n   198→\n   199→    # Phase H6b: Do NOT force detector_pivot=BEAM\n   200→    # Let the pivot be determined by the CLI precedence rules\n   201→    # (custom vectors imply SAMPLE pivot regardless of -distance)\n   202→    detector_config = DetectorConfig(\n   203→        spixels=detpixels[0],\n   204→        fpixels=detpixels[1],\n   205→        pixel_size_mm=pixel_size,\n   206→        distance_mm=distance,\n   207→        detector_convention=DetectorConvention.CUSTOM,\n   208→        beam_center_s=Ybeam,  # Ybeam maps to slow axis\n   209→        beam_center_f=Xbeam,  # Xbeam maps to fast axis\n   210→        pix0_override_m=tuple(pix0_vector_m),\n   211→        custom_fdet_vector=tuple(fdet_vector),\n   212→        custom_sdet_vector=tuple(sdet_vector),\n   213→        custom_odet_vector=tuple(odet_vector),\n   214→        custom_beam_vector=tuple(beam_vector),  # Phase H1: Pass beam_vector to Detector\n   215→        oversample=oversample\n   216→        # Phase H6b: Omit detector_pivot to let DetectorConfig choose SAMPLE\n   217→    )\n   218→\n   219→    # Create beam config\n   220→    # Note: beam_vector is stored separately and set in simulator based on convention\n   221→    beam_config = BeamConfig(\n   222→        wavelength_A=wavelength_A,\n   223→        fluence=fluence,\n   224→        polarization_axis=(0.0, 1.0, 0.0),  # Default from C code\n   225→        polarization_factor=1.0,\n   226→        nopolar=False\n   227→    )\n   228→\n   229→    # Create models\n   230→    crystal = Crystal(crystal_config, device='cpu', dtype=torch.float64)\n   231→    crystal.load_hkl(str(hkl_file), write_cache=False)\n   232→\n   233→    # Phase H6b: Detector creation will now emit TRACE_PY lines via patched __init__\n   234→    detector = Detector(detector_config, device='cpu', dtype=torch.float64)\n   235→\n   236→    # Create simulator with trace enabled\n   237→    debug_config = {\n   238→        'trace_pixel': target_pixel  # [slow, fast]\n   239→    }\n   240→\n   241→    simulator = Simulator(\n   242→        crystal=crystal,\n   243→        detector=detector,\n   244→        crystal_config=crystal_config,\n   245→        beam_config=beam_config,\n   246→        device='cpu',\n   247→        dtype=torch.float64,\n   248→        debug_config=debug_config\n   249→    )\n   250→\n   251→    # Phase H1: No manual incident beam override\n   252→    # Detector.apply_custom_vectors() now consumes beam_vector from custom_beam_direction during init\n   253→    # This allows end-to-end CLI→Detector flow without manual patches\n   254→\n   255→    # Run simulation (trace will be printed to stdout)\n   256→    print(f\"# Tracing pixel (slow={target_pixel[0]}, fast={target_pixel[1]})\", file=sys.stderr)\n   257→    image = simulator.run()\n   258→    print(f\"# Trace complete\", file=sys.stderr)\n   259→\n   260→if __name__ == '__main__':\n   261→    main()\n   262→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it looks malicious. If it does, you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer high-level questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"177208b5-7a33-4e12-bfc1-40b3148bf72c","uuid":"5d25e887-2997-4af2-8110-a44ed12f39b8"}
