          return torch.full_like(h, float(self.config.default_F), device=self.device, dtype=self.dtype)\n   206â†’\n   207â†’        h_min = self.hkl_metadata['h_min']\n   208â†’        h_max = self.hkl_metadata['h_max']\n   209â†’        k_min = self.hkl_metadata['k_min']\n   210â†’        k_max = self.hkl_metadata['k_max']\n   211â†’        l_min = self.hkl_metadata['l_min']\n   212â†’        l_max = self.hkl_metadata['l_max']\n   213â†’\n   214â†’        if self.interpolate:\n   215â†’            # Tricubic interpolation (AT-STR-002)\n   216â†’            return self._tricubic_interpolation(h, k, l)\n   217â†’        else:\n   218â†’            # Nearest-neighbor lookup\n   219â†’            return self._nearest_neighbor_lookup(h, k, l)\n   220â†’\n   221â†’    def _nearest_neighbor_lookup(\n   222â†’        self, h: torch.Tensor, k: torch.Tensor, l: torch.Tensor  # noqa: E741\n   223â†’    ) -> torch.Tensor:\n   224â†’        \"\"\"Nearest-neighbor structure factor lookup (AT-STR-001).\"\"\"\n   225â†’        if self.hkl_metadata is None:\n   226â†’            return torch.full_like(h, float(self.config.default_F), device=self.device, dtype=self.dtype)\n   227â†’\n   228â†’        h_min = self.hkl_metadata['h_min']\n   229â†’        h_max = self.hkl_metadata['h_max']\n   230â†’        k_min = self.hkl_metadata['k_min']\n   231â†’        k_max = self.hkl_metadata['k_max']\n   232â†’        l_min = self.hkl_metadata['l_min']\n   233â†’        l_max = self.hkl_metadata['l_max']\n   234â†’\n   235â†’        # Round to nearest integers (h0, k0, l0 in C code)\n   236â†’        h_int = torch.round(h).long()\n   237â†’        k_int = torch.round(k).long()\n   238â†’        l_int = torch.round(l).long()\n   239â†’\n   240â†’        # Check bounds\n   241â†’        in_bounds = (\n   242â†’            (h_int >= h_min) & (h_int <= h_max) &\n   243â†’            (k_int >= k_min) & (k_int <= k_max) &\n   244â†’            (l_int >= l_min) & (l_int <= l_max)\n   245â†’        