356â†’            # Disable interpolation permanently\n   357â†’            self.interpolate = False\n   358â†’\n   359â†’            # Return default_F for this evaluation\n   360â†’            return torch.full_like(h, float(self.config.default_F), device=self.device, dtype=self.dtype)\n   361â†’\n   362â†’        # Phase C1: Batched Neighborhood Gather Implementation\n   363â†’        # Following design_notes.md Section 2: flatten all batch dimensions, build (B,4,4,4) neighborhoods\n   364â†’\n   365â†’        # Store original shape for final reshape\n   366â†’        original_shape = h.shape\n   367â†’\n   368â†’        # Flatten all dimensions to (B,) for batched processing\n   369â†’        h_flat = h.reshape(-1)\n   370â†’        k_flat = k.reshape(-1)\n   371â†’        l_flat = l.reshape(-1)\n   372â†’        B = h_flat.shape[0]\n   373â†’\n   374â†’        # Recompute floor indices for flattened tensors\n   375â†’        h_flr_flat = torch.floor(h_flat).long()\n   376â†’        k_flr_flat = torch.floor(k_flat).long()\n   377â†’        l_flr_flat = torch.floor(l_flat).long()\n   378â†’\n   379â†’        # Build offset array [-1, 0, 1, 2] for neighborhood gathering\n   380â†’        offsets = torch.arange(-1, 3, device=self.device, dtype=torch.long)  # (4,)\n   381â†’\n   382â†’        # Build coordinate grids for each query point: (B, 4)\n   383â†’        # h_grid[i] = [h_flr[i]-1, h_flr[i], h_flr[i]+1, h_flr[i]+2]\n   384â†’        h_grid_coords = h_flr_flat.unsqueeze(-1) + offsets  # (B, 4)\n   385â†’        k_grid_coords = k_flr_flat.unsqueeze(-1) + offsets  # (B, 4)\n   386â†’        l_grid_coords = l_flr_flat.unsqueeze(-1) + offsets  # (B, 4)\n   387â†’\n   388â†’        # Build the batched 4x4x4 subcubes of structure factors\n   389â†’        if self.hkl_data is None:\n   390â†’            # If no HKL data loaded, use default_F everywhere: shape (B, 4, 4, 4)\n   391â†’            sub_Fhkl = torch.full((B, 4, 4, 4), self.config.default_F, dtype=self.dtype, device=self.device)\n   392â†’            # Coordinate arrays for polin3 (float, for interpolation): (B, 4)\n   393â†’            h_indices = h_grid_coords.to(dtype=self.dtype)\n   394â†’            k_indices = k_grid_coords.to(dtype=self.dtype)\n   395â†’            l_indices = l_grid_coords.to(dtype=self.dtype)\n   396â†’        else:\n   397â†’            # Convert to array indices (relative to hkl_data origin)\n   398â†’            h_array_grid = h_grid_coords - h_min  # (B, 4)\n   399â†’            k_array_grid = k_grid_coords - k_min  # (B, 4)\n   400â†’            l_array_grid = l_grid_coords - l_min  # (B, 4)\n   401â†’\n   402â†’            # Advanced indexing to build (B, 4, 4, 4) neighborhoods\n   403â†’            # Following design_notes.md Section 2.6 broadcasting pattern:\n   404â†’            # h_array_grid[:, :, None, None] â†’ (B, 4, 1, 1)\n   405â†’            # k_array_grid[:, None, :, None] â†’ (B, 1, 4, 1)\n   406â†’            # l_array_grid[:, None, None, :] â†’ (B, 1, 1, 4)\n   407â†’            # Result: (B, 4, 4, 4)\n   408â†’            sub_Fhkl = self.hkl_data[\n   409â†’                h_array_grid[:, :, None, None],\n   410â†’                k_array_grid[:, None, :, None],\n   411â†’                l_array_grid[:, None, None, :]\n   412â†’            ]\n   413â†’\n   414â†’            # Coordinate arrays for polin3 (float Miller indices): (B, 4)\n   415â†’            h_indices = h_grid_coords.to(dtype=self.dtype)\n   416â†’            k_indices = k_grid_coords.to(dtype=self.dtype)\n   417â†’            l_indices = l_grid_coords.to(dtype=self.dtype)\n   418â†’\n   419â†’        # Phase C3: Shape assertions to prevent silent regressions\n   420â†’        # Verify neighborhood tensor has correct shape for polynomial evaluation\n   421â†’        assert sub_Fhkl.shape == (B, 4, 4, 4), \\\n   422â†’            f\"Neighborhood shape mismatch: expected ({B}, 4, 4, 4), got {sub_Fhkl.shape}\"\n   423â†’\n   424â†’        # Verify coordinate 