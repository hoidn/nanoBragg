ssert crystal._interpolation_warning_shown, \"Warning flag should remain set\"\n   287â†’        assert not crystal.interpolate, \"Interpolation should remain disabled\"\n   288â†’\n   289â†’        # Third query (in-bounds but interpolation disabled): should use nearest-neighbor\n   290â†’        h_valid = torch.tensor([1.5], dtype=torch.float32)\n   291â†’        k_valid = torch.tensor([0.0], dtype=torch.float32)\n   292â†’        l_valid = torch.tensor([0.0], dtype=torch.float32)\n   293â†’\n   294â†’        captured_output3 = io.StringIO()\n   295â†’        sys.stdout = captured_output3\n   296â†’\n   297â†’        F_third = crystal._tricubic_interpolation(h_valid, k_valid, l_valid)\n   298â†’\n   299â†’        sys.stdout = sys.__stdout__\n   300â†’        warning_text3 = captured_output3.getvalue()\n   301â†’\n   302â†’        # Should not trigger OOB warning (in-bounds) but uses nearest-neighbor due to disabled interpolation\n   303â†’        assert \"out of range\" not in warning_text3, \\\n   304â†’            \"In-bounds query should not mention out of range\"\n   305â†’\n   306â†’        # Nearest-neighbor should return one of the grid values (50.0 in this case)\n   307â†’        # Since interpolation is disabled, we expect the nearest neighbor value\n   308â†’        assert F_third.item() == 50.0 or F_third.item() == expected_default, \\\n   309â†’            f\"Disabled interpolation should use nearest-neighbor or default_F, got {F_third.item()}\"\n   310â†’\n   311â†’        print(\"âœ“ Phase C2: OOB warning single-fire behavior validated\")\n   312â†’        print(f\"  - First OOB: warning printed, interpolation disabled, returned default_F={expected_default}\")\n   313â†’        print(f\"  - Second OOB: no warning, still returns default_F={expected_default}\")\n   314â†’        print(f\"  - In-bounds post-disable: uses fallback path, returns {F_third.item()}\")\n   315â†’\n   316â†’    @pytest.mark.parametrize(\"device\", [\n   317â†’        \"cpu\",\