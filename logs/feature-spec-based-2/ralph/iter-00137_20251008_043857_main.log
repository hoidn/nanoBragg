{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_018Urcsi7tkZrjtXeXR896PF","type":"tool_result","content":"     1â†’#!/usr/bin/env python\n     2â†’\"\"\"\n     3â†’Phase L2b PyTorch Scaling Trace Harness\n     4â†’\n     5â†’Captures the PyTorch side of the scaling chain comparison for supervisor command.\n     6â†’Mirrors the C trace fields from reports/2025-10-cli-flags/phase_l/scaling_audit/c_trace_scaling.log.\n     7â†’\n     8â†’Date: 2025-10-17\n     9â†’Engineer: Ralph (loop i=62, evidence-only mode)\n    10â†’Plan Reference: plans/active/cli-noise-pix0/plan.md Phase L2b\n    11â†’\"\"\"\n    12â†’\n    13â†’import argparse\n    14â†’import json\n    15â†’import sys\n    16â†’import platform\n    17â†’import os\n    18â†’import torch\n    19â†’from pathlib import Path\n    20â†’from datetime import datetime\n    21â†’\n    22â†’# Set KMP env before torch import (defensive re-check)\n    23â†’os.environ['KMP_DUPLICATE_LIB_OK'] = 'TRUE'\n    24â†’\n    25â†’# Ensure editable install is used\n    26â†’if 'nanobrag_torch' not in sys.modules:\n    27â†’    # PYTHONPATH=src should be set externally per input.md How-To Step 3\n    28â†’    pass\n    29â†’\n    30â†’from nanobrag_torch.config import CrystalConfig, DetectorConfig, BeamConfig, DetectorConvention, DetectorPivot\n    31â†’from nanobrag_torch.models.detector import Detector\n    32â†’from nanobrag_torch.models.crystal import Crystal\n    33â†’from nanobrag_torch.simulator import Simulator\n    34â†’from nanobrag_torch.io.mosflm import read_mosflm_matrix, reciprocal_to_real_cell\n    35â†’from nanobrag_torch.io.hkl import read_hkl_file\n    36â†’\n    37â†’\n    38â†’def create_parser():\n    39â†’    \"\"\"CLI for trace harness per input.md Step 2.\"\"\"\n    40â†’    parser = argparse.ArgumentParser(description=\"PyTorch scaling trace harness\")\n    41â†’    parser.add_argument('--pixel', type=int, nargs=2, required=True, metavar=('SLOW', 'FAST'),\n    42â†’                        help='Pixel coordinates to trace (slow, fast)')\n    43â†’    parser.add_argument('--out', type=str, required=True,\n    44â†’                        help='Output trace filename (e.g., trace_py_scaling.log)')\n    45â†’    parser.add_argument('--config', type=str, choices=['supervisor'], required=True,\n    46â†’                        help='Config preset to use (supervisor = CLI-FLAGS-003 supervisor command)')\n    47â†’    parser.add_argument('--device', type=str, default='cpu',\n    48â†’                        help='Device (cpu or cuda)')\n    49â†’    parser.add_argument('--dtype', type=str, default='float32', choices=['float32', 'float64'],\n    50â†’                        help='Tensor dtype (default float32)')\n    51â†’    return parser\n    52â†’\n    53â†’\n    54â†’def get_supervisor_params(dtype, device):\n    55â†’    \"\"\"\n    56â†’    Return supervisor command parameter dict (not yet instantiated as config objects).\n    57â†’\n    58â†’    Per input.md Step 3.\n    59â†’    \"\"\"\n    60â†’    # Supervisor command parameters (from phase_i/supervisor_command/README.md:33-75)\n    61â†’    return {\n    62â†’        'crystal': {\n    63â†’            'misset_deg': (0.0, 0.0, 0.0),\n    64â†’            'phi_start_deg': 0.0,\n    65â†’            'osc_range_deg': 0.1,\n    66â†’            'phi_steps': 10,\n    67â†’            'spindle_axis': torch.tensor([-1.0, 0.0, 0.0], dtype=dtype, device=device),\n    68â†’            'mosaic_spread_deg': 0.0,\n    69â†’            'mosaic_domains': 1,\n    70â†’            'mosaic_seed': -12345678,\n    71â†’            'N_cells': (36, 47, 29),\n    72â†’            'default_F': 0.0  # Using HKL file\n    73â†’        },\n    74â†’        'detector': {\n    75â†’            'distance_mm': 231.274660,\n    76â†’            'pixel_size_mm': 0.172,\n    77â†’            'spixels': 2527,\n    78â†’            'fpixels': 2463,\n    79â†’            'beam_center_s': 213.907080,  # Ybeam in mm\n    80â†’            'beam_center_f': 217.742295,  # Xbeam in mm\n    81â†’            'detector_rotx_deg': 0.0,\n    82â†’            'detector_roty_deg': 0.0,\n    83â†’            'detector_rotz_deg': 0.0,\n    84â†’            'detector_twotheta_deg': 0.0,\n    85â†’            'detector_convention': DetectorConvention.CUSTOM,\n    86â†’            'detector_pivot': DetectorPivot.SAMPLE,\n    87â†’            'oversample': 1,\n    88â†’            # Custom detector vectors (per supervisor command)\n    89â†’            'custom_fdet_vector': (0.999982, -0.005998, -0.000118),\n    90â†’            'custom_sdet_vector': (-0.005998, -0.999970, -0.004913),\n    91â†’            'custom_odet_vector': (-0.000088, 0.004914, -0.999988),\n    92â†’            'custom_beam_vector': (0.00051387949, 0.0, -0.99999986),\n    93â†’            # Custom pix0 override (mmâ†’m conversion)\n    94â†’            'pix0_override_m': (-0.216336293, 0.215205512, -0.230200866)\n    95â†’        },\n    96â†’        'beam': {\n    97â†’            'wavelength_A': 0.9768,\n    98â†’            'polarization_factor': 0.0,  # Kahn factor = 0.0 per c_stdout line 132\n    99â†’            'flux': 1e18,\n   100â†’            'exposure': 1.0,\n   101â†’            'beamsize_mm': 1.0\n   102â†’        }\n   103â†’    }\n   104â†’\n   105â†’\n   106â†’\n   107â†’def main():\n   108â†’    parser = create_parser()\n   109â†’    args = parser.parse_args()\n   110â†’\n   111â†’    # Resolve dtype and device (Step 3)\n   112â†’    dtype = torch.float32 if args.dtype == 'float32' else torch.float64\n   113â†’    device = torch.device(args.device)\n   114â†’\n   115â†’    # Log environment (Step 9 per input.md)\n   116â†’    env_info = {\n   117â†’        'python_version': sys.version,\n   118â†’        'torch_version': torch.__version__,\n   119â†’        'platform': platform.platform(),\n   120â†’        'cuda_available': torch.cuda.is_available(),\n   121â†’        'device': str(device),\n   122â†’        'dtype': args.dtype,\n   123â†’        'git_sha': os.popen('git rev-parse HEAD').read().strip(),\n   124â†’        'timestamp_iso': datetime.utcnow().isoformat() + 'Z'\n   125â†’    }\n   126â†’\n   127â†’    env_path = Path('reports/2025-10-cli-flags/phase_l/scaling_audit/trace_py_env.json')\n   128â†’    with open(env_path, 'w') as f:\n   129â†’        json.dump(env_info, f, indent=2)\n   130â†’\n   131â†’    # Load configuration parameters (Step 3)\n   132â†’    if args.config == 'supervisor':\n   133â†’        params = get_supervisor_params(dtype, device)\n   134â†’    else:\n   135â†’        raise ValueError(f\"Unknown config: {args.config}\")\n   136â†’\n   137â†’    # Load MOSFLM matrix (Step 3)\n   138â†’    mat_path = Path('A.mat')\n   139â†’    if not mat_path.exists():\n   140â†’        raise FileNotFoundError(f\"A.mat not found in current directory: {Path.cwd()}\")\n   141â†’\n   142â†’    # Note: wavelength is already in params['beam']['wavelength_A']\n   143â†’    a_star, b_star, c_star = read_mosflm_matrix(str(mat_path), params['beam']['wavelength_A'])\n   144â†’\n   145â†’    # Extract cell parameters from reciprocal vectors\n   146â†’    cell_params = reciprocal_to_real_cell(a_star, b_star, c_star)\n   147â†’    cell_a, cell_b, cell_c, cell_alpha, cell_beta, cell_gamma = cell_params\n   148â†’\n   149â†’    # Build crystal config with cell params and MOSFLM vectors\n   150â†’    # Each MOSFLM reciprocal vector must be assigned to its own config field\n   151â†’    # (not packed into a single list) so Crystal uses the specified orientation\n   152â†’    crystal_config = CrystalConfig(\n   153â†’        cell_a=cell_a,\n   154â†’        cell_b=cell_b,\n   155â†’        cell_c=cell_c,\n   156â†’        cell_alpha=cell_alpha,\n   157â†’        cell_beta=cell_beta,\n   158â†’        cell_gamma=cell_gamma,\n   159â†’        mosflm_a_star=torch.tensor(a_star, dtype=dtype, device=device),\n   160â†’        mosflm_b_star=torch.tensor(b_star, dtype=dtype, device=device),\n   161â†’        mosflm_c_star=torch.tensor(c_star, dtype=dtype, device=device),\n   162â†’        **params['crystal']\n   163â†’    )\n   164â†’\n   165â†’    # Build detector and beam configs\n   166â†’    detector_config = DetectorConfig(**params['detector'])\n   167â†’    beam_config = BeamConfig(**params['beam'])\n   168â†’\n   169â†’    # Load HKL file (Step 3)\n   170â†’    hkl_path = Path('scaled.hkl')\n   171â†’    if not hkl_path.exists():\n   172â†’        raise FileNotFoundError(f\"scaled.hkl not found in current directory: {Path.cwd()}\")\n   173â†’\n   174â†’    # Read HKL grid (returns tensor + metadata dict)\n   175â†’    F_grid_tensor, hkl_metadata = read_hkl_file(str(hkl_path), default_F=0.0, device=device, dtype=dtype)\n   176â†’\n   177â†’    # Extract metadata for crystal configuration\n   178â†’    h_min = hkl_metadata['h_min']\n   179â†’    h_max = hkl_metadata['h_max']\n   180â†’    k_min = hkl_metadata['k_min']\n   181â†’    k_max = hkl_metadata['k_max']\n   182â†’    l_min = hkl_metadata['l_min']\n   183â†’    l_max = hkl_metadata['l_max']\n   184â†’    h_range = h_max - h_min + 1\n   185â†’    k_range = k_max - k_min + 1\n   186â†’    l_range = l_max - l_min + 1\n   187â†’\n   188â†’    # Snapshot config (Step 4 per input.md)\n   189â†’    config_snapshot = {\n   190â†’        'crystal': {\n   191â†’            'N_cells': crystal_config.N_cells,\n   192â†’            'phi_start_deg': crystal_config.phi_start_deg,\n   193â†’            'osc_range_deg': crystal_config.osc_range_deg,\n   194â†’            'phi_steps': crystal_config.phi_steps,\n   195â†’            'mosaic_spread_deg': crystal_config.mosaic_spread_deg,\n   196â†’            'mosaic_domains': crystal_config.mosaic_domains,\n   197â†’            'spindle_axis': crystal_config.spindle_axis.tolist()\n   198â†’        },\n   199â†’        'detector': {\n   200â†’            'distance_mm': detector_config.distance_mm,\n   201â†’            'pixel_size_mm': detector_config.pixel_size_mm,\n   202â†’            'spixels': detector_config.spixels,\n   203â†’            'fpixels': detector_config.fpixels,\n   204â†’            'beam_center_s': detector_config.beam_center_s,\n   205â†’            'beam_center_f': detector_config.beam_center_f,\n   206â†’            'convention': str(detector_config.detector_convention),\n   207â†’            'pivot': str(detector_config.detector_pivot),\n   208â†’            'oversample': detector_config.oversample\n   209â†’        },\n   210â†’        'beam': {\n   211â†’            'wavelength_A': beam_config.wavelength_A,\n   212â†’            'polarization_factor': beam_config.polarization_factor,\n   213â†’            'flux': beam_config.flux,\n   214â†’            'exposure': beam_config.exposure,\n   215â†’            'beamsize_mm': beam_config.beamsize_mm\n   216â†’        },\n   217â†’        'hkl': {\n   218â†’            'path': str(hkl_path),\n   219â†’            'h_range': (h_min, h_min + h_range),\n   220â†’            'k_range': (k_min, k_min + k_range),\n   221â†’            'l_range': (l_min, l_min + l_range)\n   222â†’        }\n   223â†’    }\n   224â†’\n   225â†’    config_path = Path('reports/2025-10-cli-flags/phase_l/scaling_audit/config_snapshot.json')\n   226â†’    with open(config_path, 'w') as f:\n   227â†’        json.dump(config_snapshot, f, indent=2)\n   228â†’\n   229â†’    # Instantiate components (Step 5 per input.md, honour -nonoise)\n   230â†’    detector = Detector(detector_config, dtype=dtype, device=device)\n   231â†’    crystal = Crystal(crystal_config, dtype=dtype, device=device)\n   232â†’\n   233â†’    # Attach HKL grid and metadata to Crystal (Phase L2b fix)\n   234â†’    # Crystal.get_structure_factor consults hkl_data and hkl_metadata, not ad-hoc attributes\n   235â†’    crystal.hkl_data = F_grid_tensor\n   236â†’    crystal.hkl_metadata = hkl_metadata\n   237â†’\n   238â†’    # Run simulator with debug_config to capture live TRACE_PY output (Step 5)\n   239â†’    # Per Attempt #67 (2025-10-06): Simulator with debug_config={'trace_pixel': [s, f]}\n   240â†’    # automatically emits TRACE_PY lines to stdout containing real computed values\n   241â†’    slow, fast = args.pixel\n   242â†’    debug_config = {'trace_pixel': [slow, fast]}\n   243â†’\n   244â†’    simulator = Simulator(\n   245â†’        crystal, detector,\n   246â†’        beam_config=beam_config,\n   247â†’        dtype=dtype, device=device,\n   248â†’        debug_config=debug_config\n   249â†’    )\n   250â†’\n   251â†’    # Capture stdout during simulation run (Step 6 per input.md)\n   252â†’    # Use contextlib.redirect_stdout to capture TRACE_PY output\n   253â†’    import io\n   254â†’    from contextlib import redirect_stdout\n   255â†’\n   256â†’    stdout_capture = io.StringIO()\n   257â†’    with redirect_stdout(stdout_capture):\n   258â†’        intensities = simulator.run()\n   259â†’\n   260â†’    # Extract TRACE_PY lines from captured output (Step 7)\n   261â†’    trace_lines = []\n   262â†’    trace_py_phi_lines = []\n   263â†’    for line in stdout_capture.getvalue().splitlines():\n   264â†’        if line.startswith('TRACE_PY_PHI'):\n   265â†’            trace_py_phi_lines.append(line)\n   266â†’        elif line.startswith('TRACE_PY:'):\n   267â†’            trace_lines.append(line)\n   268â†’\n   269â†’    if not trace_lines:\n   270â†’        raise RuntimeError(\"No TRACE_PY output captured. Simulator may not have emitted trace for pixel.\")\n   271â†’\n   272â†’    # Write main trace to file (Step 8)\n   273â†’    trace_path = Path(args.out)\n   274â†’    trace_path.parent.mkdir(parents=True, exist_ok=True)\n   275â†’    with open(trace_path, 'w') as f:\n   276â†’        f.write('\\n'.join(trace_lines))\n   277â†’        f.write('\\n')\n   278â†’\n   279â†’    # Write per-Ï† trace lines if captured\n   280â†’    if trace_py_phi_lines:\n   281â†’        phi_trace_name = args.out.replace('.log', '_per_phi.log')\n   282â†’        phi_trace_path = Path('reports/2025-10-cli-flags/phase_l/per_phi') / phi_trace_name\n   283â†’        phi_trace_path.parent.mkdir(parents=True, exist_ok=True)\n   284â†’\n   285â†’        with open(phi_trace_path, 'w') as f:\n   286â†’            f.write('\\n'.join(trace_py_phi_lines))\n   287â†’            f.write('\\n')\n   288â†’\n   289â†’        # Parse per-Ï† lines into structured JSON\n   290â†’        per_phi_data = []\n   291â†’        for line in trace_py_phi_lines:\n   292â†’            # Parse: TRACE_PY_PHI phi_tic=0 phi_deg=0 k_frac=-3.857... F_latt_b=1.779... F_latt=-0.0625...\n   293â†’            parts = line.split()\n   294â†’            if len(parts) >= 6:\n   295â†’                entry = {}\n   296â†’                for part in parts[1:]:  # Skip \"TRACE_PY_PHI\" prefix\n   297â†’                    if '=' in part:\n   298â†’                        key, value = part.split('=', 1)\n   299â†’                        # Convert to appropriate type\n   300â†’                        if key in ['phi_tic']:\n   301â†’                            entry[key] = int(value)\n   302â†’                        else:\n   303â†’                            entry[key] = float(value)\n   304â†’                per_phi_data.append(entry)\n   305â†’\n   306â†’        # Write structured JSON\n   307â†’        json_name = args.out.replace('.log', '_per_phi.json')\n   308â†’        json_path = Path('reports/2025-10-cli-flags/phase_l/per_phi') / json_name\n   309â†’\n   310â†’        timestamp = datetime.utcnow().isoformat() + 'Z'\n   311â†’        json_output = {\n   312â†’            'timestamp': timestamp,\n   313â†’            'pixel': {'slow': slow, 'fast': fast},\n   314â†’            'config': args.config,\n   315â†’            'device': str(device),\n   316â†’            'dtype': args.dtype,\n   317â†’            'per_phi_entries': per_phi_data\n   318â†’        }\n   319â†’\n   320â†’        with open(json_path, 'w') as f:\n   321â†’            json.dump(json_output, f, indent=2)\n   322