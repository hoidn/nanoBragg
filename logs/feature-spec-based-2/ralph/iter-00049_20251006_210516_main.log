rt metrics\n   129â†’            metrics = {\n   130â†’                'c_sum': float(c_sum),\n   131â†’                'py_sum': float(py_sum),\n   132â†’                'sum_ratio': float(sum_ratio),\n   133â†’                'c_max': float(c_max),\n   134â†’                'py_max': float(py_max),\n   135â†’                'max_ratio': float(max_ratio),\n   136â†’                'correlation': float(correlation)\n   137â†’            }\n   138â†’\n   139â†’            # Save metrics on failure for debugging\n   140â†’            if abs(sum_ratio - 1.0) > tolerance or abs(max_ratio - 1.0) > tolerance or correlation < 0.999:\n   141â†’                report_dir = Path('reports/2025-10-cli-flags/phase_k/f_latt_fix')\n   142â†’                report_dir.mkdir(parents=True, exist_ok=True)\n   143â†’                metrics_path = report_dir / 'test_metrics_failure.json'\n   144â†’                with open(metrics_path, 'w') as f:\n   145â†’                    json.dump(metrics, f, indent=2)\n   146â†’\n   147â†’                # Save images for inspection\n   148â†’                np.save(report_dir / 'c_image.npy', c_img)\n   149â†’                np.save(report_dir / 'py_image.npy', py_img)\n   150â†’\n   151â†’            # Assertions\n   152â†’            assert correlation >= 0.999, \\\n   153â†’                f\"Correlation {correlation:.6f} < 0.999. Metrics: {json.dumps(metrics, indent=2)}\"\n   154â†’\n   155â†’            assert abs(sum_ratio - 1.0) <= tolerance, \\\n   156â†’                f\"Sum ratio {sum_ratio:.6f} deviates by {abs(sum_ratio - 1.0):.6f} (> {tolerance}). \" \\\n   157â†’                f\"C sum={c_sum:.6g}, PyTorch sum={py_sum:.6g}. Metrics: {json.dumps(metrics, indent=2)}\"\n   158â†’\n   159â†’            assert abs(max_ratio - 1.0) <= tolerance, \\\n   160â†’                f\"Max ratio {max_ratio:.6f} deviates by {abs(max_ratio - 1.0):.6f} (> {tolerance}). \" \\\n   161â†’                f\"C max={c_max:.6g}, PyTorch max={py_max:.6g}.