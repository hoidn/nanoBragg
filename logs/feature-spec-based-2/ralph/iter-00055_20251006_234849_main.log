ry image.\"\"\"\n    48â†’    data = np.fromfile(path, dtype=np.float32)\n    49â†’    if data.size != shape[0] * shape[1]:\n    50â†’        raise ValueError(f\"Expected {shape[0]}Ã—{shape[1]}={shape[0]*shape[1]} floats, got {data.size}\")\n    51â†’    return data.reshape(shape)\n    52â†’\n    53â†’\n    54â†’class TestMOSFLMCellVectors:\n    55â†’    \"\"\"Test MOSFLM matrix cell vector computation (Phase K3g2).\"\"\"\n    56â†’\n    57â†’    def test_mosflm_cell_vectors(self):\n    58â†’        \"\"\"\n    59â†’        Verify that PyTorch correctly computes real-space vectors from MOSFLM matrices.\n    60â†’\n    61â†’        This test validates CLI-FLAGS-003 Phase K3g1 implementation:\n    62â†’        When MOSFLM reciprocal vectors are provided (via -mat file), PyTorch must:\n    63â†’        1. Compute V_star = a* Â· (b* Ã— c*)\n    64â†’        2. Compute V_cell = 1 / V_star\n    65â†’        3. Derive real vectors: a = (b* Ã— c*) Ã— V_cell, etc.\n    66â†’        4. Update cell_a/b/c parameters from the derived vectors\n    67â†’\n    68â†’        Expected output (from C trace):\n    69â†’        - V_cell â‰ˆ 24682.3 Ã…Â³\n    70â†’        - |a| â‰ˆ 26.7514 Ã…\n    71â†’        - |b| â‰ˆ 31.3100 Ã…\n    72â†’        - |c| â‰ˆ 33.6734 Ã…\n    73â†’        \"\"\"\n    74â†’        import torch\n    75â†’        from nanobrag_torch.models.crystal import Crystal, CrystalConfig\n    76â†’        from nanobrag_torch.io.mosflm import read_mosflm_matrix\n    77â†’\n    78â†’        # Read A.mat from repo root\n    79â†’        mat_file = Path('A.mat')\n    80â†’        if not mat_file.exists():\n    81â†’            pytest.skip(\"A.mat not found in repository root\")\n    82â†’\n    83â†’        wavelength_A = 0.976800\n    84â†’        a_star, b_star, c_star = read_mosflm_matrix(str(mat_file), wavelength_A)\n    85â†’\n    86â†’        # Create Crystal with MOSFLM orientation\n    87â†’        config = CrystalConfig(\n    88â†’            cell_a=100.0,  # placeholder\n    89â†’            cell_b=100.0,\n    90â†’            cell_c=100.0,\n    91â†’            cell_alpha=90.0,\n    92â†’            cell_beta=90.0,\n    93â†’            cell_gamma=90.0,\n    94â†’            mosflm_a_star=a_star,\n    95â†’            mosflm_b_star=b_star,\n    96â†’            mosflm_c_star=c_star,\n    97â†’            N_cells=(36, 47, 29),\n    98â†’        )\n    99â†’\n   100â†’        crystal = Crystal(config, device=torch.device('cpu'), dtype=torch.float64)\n   101â†’        tensors = crystal.compute_cell_tensors()\n   102â†’\n   103â†’        # Expected values from C trace (reports/2025-10-cli-flags/phase_k/base_lattice/c_stdout.txt)\n   104â†’        expected_V_cell = 24682.3  # Ã…Â³\n   105â†’        expected_mag_a = 26.7514   # Ã…\n   106â†’        expected_mag_b = 31.3100   # Ã…\n   107â†’        expected_mag_c = 33.6734   # Ã…\n   108â†’\n   109â†’        # Compute actual values\n   110â†’        actual_V_cell = tensors['V'].item()\n   111â†’        actual_mag_a = torch.norm(tensors['a']).item()\n   112â†’        actual_mag_b = torch.norm(tensors['b']).item()\n   113â†’        actual_mag_c = torch.norm(tensors['c']).item()\n   114â†’\n   115â†’        # Tolerance: 5e-4 relative error (from Phase K3f exit criteria)\n   116â†’        rtol = 5e-4\n   117â†’\n   118â†’        # Assertions\n   119â†’        V_rel_error = abs(actual_V_cell - expected_V_cell) / expected_V_cell\n   120â†’        assert V_rel_error <= rtol, \\\n   121â†’            f\"V_cell relative error {V_rel_error:.6g} > {rtol}. \" \\\n   122â†’            f\"Expected {expected_V_cell:.2f}, got {actual_V_cell:.2f} Ã…Â³\"\n   123â†’\n   124â†’        a_rel_error = abs(actual_mag_a - expected_mag_a) / expected_mag_a\n   125â†’        assert a_rel_error <= rtol, \\\n   126â†’            f\"|a| relative error {a_rel_error:.6g} > {rtol}. \" \\\n   127â†’            f\"Expected {expected_mag_a:.4f}, got {actual_mag_a:.4f} Ã…\"\n   128â†’\n   129â†’        b_rel_error = abs(actual_mag_b - expected_mag_b) / expected_mag_b\n   130â†’        assert b_rel_error <= rtol, \\\n   131â†’            f\"|b| relative error {b_rel_error:.6g} > {rtol}. \" \\\n   132â†’            f\"Expected {expected_mag_b:.4f}, got {actual_mag_b:.4f} Ã…\"\n   133â†’\n   134â†’        c_rel_error = abs(actual_mag_c - expected_mag_c) / expected_mag_c\n   135â†’        assert c_rel_error <= rtol, \\\n   136â†’            f\"|c| relative error {c_rel_error:.6g} > {rtol}. \" \\\n   137â†’            f\"Expected {expected_mag_c:.4f}, got {actual_mag_c:.4f} Ã…\"\n   138â†’\n   139â†’\n   140â†’@pytest.mark.skipif(not is_parallel_enabled(), reason=\"NB_RUN_PARALLEL=1 required for Câ†”PyTorch parity\")\n   141â†’class TestFlattSquareMatchesC:\n   142â†’    \"\"\"Test that SQUARE lattice factor matches C implementation.\"\"\"\n   143â†’\n   144â†’    def test_f_latt_square_matches_c(self):\n   145â†’        \"\"\"\n   146â†’        Verify F_latt and I_before_scaling match between C and PyTorch for SQUARE shape.\n   147â†’\n   148â†’        This test runs a minimal SQUARE crystal simulation and compares final intensities.\n   149â†’        The ratio should be within 1e-3 (Phase K3 exit criterion).\n   150â†’\n   151â†’        CRITICAL FIX: This test validates that F_latt uses sincg(Ï€Â·h, Na) instead of\n   152â†’        the incorrect sincg(Ï€Â·(h-h0), Na) which caused a 463Ã— error (Attempt #28).\n   153â†’        \"\"\"\n   154â†’        if not is_parallel_enabled():\n   155â†’            pytest.skip(\"NB_RUN_PARALLEL=1 required\")\n   156â†’\n   157â†’        c_bin = get_c_binary()\n   158â†’        py_cli = f\"{sys.executable} -m nanobrag_torch\"\n   159â†’\n   160â†’        # Shared parameters (simple cubic, SQUARE shape)\n   161â†’        common_args = [\n   162â†’            '-cell', '100', '100', '100', '90', '90', '90',\n   163â†’            '-default_F', '300',\n   164â†’            '-N', '10',\n   165â†’            '-lambda', '1.0',\n   166â†’            '-distance', '100',\n   167â†’            '-detpixels', '512',\n   168â†’            '-pixel', '0.1',\n   169â†’            '-oversample', '1',\n   170â†’            '-phisteps', '1',\n   171â†’            '-mosaic_dom', '1'\n   172â†’        ]\n   173â†’\n   174â†’        with tempfile.TemporaryDirectory() as tmpdir:\n   175â†’            tmpdir = Path(tmpdir)\n   176â†’            c_out = tmpdir / 'c_image.bin'\n   177â†’            py_out = tmpdir / 'py_image.bin'\n   178â†’\n   179â†’            # Run C\n   180â†’            c_cmd = [c_bin] + common_args + ['-floatfile', str(c_out)]\n   181â†’            result_c = subprocess.run(c_cmd, capture_output=True, text=True)\n   182â†’            if result_c.returncode != 0:\n   183â†’                pytest.fail(f\"C simulation failed:\\n{result_c.stderr}\")\n   184â†’\n   185â†’            # Run PyTorch\n   186â†’            py_cmd = py_cli.split() + common_args + ['-floatfile', str(py_out)]\n   187â†’            result_py = subprocess.run(py_cmd, capture_output=True, text=True)\n   188â†’            if result_py.returncode != 0:\n   189â†’                pytest.fail(f\"PyTorch simulation failed:\\n{result_py.stderr}\")\n   190â†’\n   191â†’            # Load images\n   192â†’            shape = (512, 512)\n   193â†’            c_img = read_float_image(c_out, shape)\n   194â†’            py_img = read_float_image(py_out, shape)\n   195â†’\n   196â†’            # Compute metrics\n   197â†’            c_sum = np.sum(c_img)\n   198â†’            py_sum = np.sum(py_img)\n   199â†’            sum_ratio = py_sum / c_sum if c_sum > 0 else float('inf')\n   200â†’\n   201â†’            c_max = np.max(c_img)\n   202â†’            py_max = np.max(py_img)\n   203â†’            max_ratio = py_max / c_max if c_max > 0 else float('inf')\n   204â†’\n   205â†’            # Correlation\n   206â†’            c_flat = c_img.flatten()\n   207â†’            py_flat = py_img.flatten()\n   208â†’            correlation = np.corrcoef(c_flat, py_flat)[0, 1] if np.std(c_flat) > 0 and np.std(py_flat) > 0 else 0.0\n   209â†’\n   210â†’            # Phase K3 exit criteria: ratios within 1e-3\n   211â†’            # (sum_ratio and max_ratio should be ~1.0)\n   212â†’            tolerance = 1e-3\n   213â†’\n   214â†’            # Report metrics\n   215â†’            metrics = {\n   216â†’                'c_sum': float(c_sum),\n   217â†’                'py_sum': float(py_sum),\n   218â†’                'sum_ratio': float(sum_ratio),\n   219â†’                'c_max': float(c_max),\n   220â†’                'py_max': float(py_max),\n   221â†’                'max_ratio': float(max_ratio),\n   222â†’                'correlation': float(correlation)\n   223â†’            }\n   224â†’\n   225â†’            # Save metrics on failure for debugging\n   226â†’            if abs(sum_ratio - 1.0) > tolerance or abs(max_ratio - 1.0) > tolerance or correlation < 0.999:\n   227â†’                report_dir = Path('reports/2025-10-cli-flags/phase_k/f_latt_fix')\n   228â†’                report_dir.mkdir(parents=True, exist_ok=True)\n   229â†’                metrics_path = report_dir / 'test_metrics_failure.json'\n   230â†’                with open(metrics_path, 'w') as f:\n   231â†’                    json.dump(metrics, f, indent=2)\n   232â†’\n   233â†’                # Save images for inspection\n   234â†’                np.save(report_dir / 'c_image.npy', c_img)\n   235â†’                np.save(report_dir / 'py_image.npy', py_img)\n   236â†’\n   237â†’            # Assertions\n   238â†’            assert correlation >= 0.999, \\\n   239â†’                f\"Correlation {correlation:.6f} < 0.999. Metrics: {json.dumps(metrics, indent=2)}\"\n   240â†’\n   241â†’            assert abs(sum_ratio - 1.0) <= tolerance, \\\n   242â†’                f\"Sum ratio {sum_ratio:.6f} deviates by {abs(sum_ratio - 1.0):.6f} (> {tolerance}). \" \\\n   243â†’                f\"C sum={c_sum:.6g}, PyTorch sum={py_sum:.6g}. Metrics: {json.dumps(metric