n   236â†’    config_path = Path('reports/2025-10-cli-flags/phase_l/rot_vector/config_snapshot.json')\n   237â†’    with open(config_path, 'w') as f:\n   238â†’        json.dump(config_snapshot, f, indent=2)\n   239â†’\n   240â†’    # Instantiate components (Step 5 per input.md, honour -nonoise)\n   241â†’    detector = Detector(detector_config, dtype=dtype, device=device)\n   242â†’    crystal = Crystal(crystal_config, dtype=dtype, device=device)\n   243â†’\n   244â†’    # Attach HKL grid and metadata to Crystal (Phase L2b fix)\n   245â†’    # Crystal.get_structure_factor consults hkl_data and hkl_metadata, not ad-hoc attributes\n   246â†’    crystal.hkl_data = F_grid_tensor\n   247â†’    crystal.hkl_metadata = hkl_metadata\n   248â†’\n   249â†’    # === PHASE L3h: MOSFLM MATRIX PROBE ===\n   250â†’    # Extract MOSFLM reciprocal vectors at key pipeline stages to diagnose\n   251â†’    # the Y-component drift identified in Phase L3f/L3g\n   252â†’\n   253â†’    mosflm_probe_output = []\n   254â†’\n   255â†’    # Stage 1: Initial MOSFLM reciprocal vectors (before misset)\n   256â†’    # These are stored in crystal.a_star_base, b_star_base, c_star_base\n   257â†’    # after compute_cell_tensors runs in Crystal.__init__\n   258â†’    if hasattr(crystal, 'a_star_base') and crystal.a_star_base is not None:\n   259â†’        mosflm_probe_output.append(f\"TRACE_PY: mosflm_a_star_initial {crystal.a_star_base[0]:.15g} {crystal.a_star_base[1]:.15g} {crystal.a_star_base[2]:.15g}\")\n   260â†’        mosflm_probe_output.append(f\"TRACE_PY: mosflm_b_star_initial {crystal.b_star_base[0]:.15g} {crystal.b_star_base[1]:.15g} {crystal.b_star_base[2]:.15g}\")\n   261â†’        mosflm_probe_output.append(f\"TRACE_PY: mosflm_c_star_initial {crystal.c_star_base[0]:.15g} {crystal.c_star_base[1]:.15g} {crystal.c_star_base[2]:.15g}\")\n   262â†’\n   263â†’    # Stage 2: Post-misset reciprocal vectors (after misset rotation)\n   264â†’    # Extract current reciprocal vectors from Crystal\n   265â†’    a_star_post_misset = crystal.a_star\n   266â†’    b_star_post_misset = crystal.b_star\n   267â†’    c_star_post_misset = crystal.c_star\n   268â†’    mosflm_probe_output.append(f\"TRACE_PY: mosflm_a_star_post_misset {a_star_post_misset[0]:.15g} {a_star_post_misset[1]:.15g} {a_star_post_misset[2]:.15g}\")\n   269â†’    mosflm_probe_output.append(f\"TRACE_PY: mosflm_b_star_post_misset {b_star_post_misset[0]:.15g} {b_star_post_misset[1]:.15g} {b_star_post_misset[2]:.15g}\")\n   270â†’    mosflm_probe_output.append(f\"TRACE_PY: mosflm_c_star_post_misset {c_star_post_misset[0]:.15g} {c_star_post_misset[1]:.15g} {c_star_post_misset[2]:.15g}\")\n   271â†’\n   272â†’    # Stage 3: Reconstructed real-space vectors (from reciprocal via cross products)\n   273â†’    # These are stored in crystal.a, b, c\n   274â†’    a_real = crystal.a\n   275â†’    b_real = crystal.b\n   276â†’    c_real = crystal.c\n   277â†’    mosflm_probe_output.append(f\"TRACE_PY: real_a_reconstructed {a_real[0]:.15g} {a_real[1]:.15g} {a_real[2]:.15g}\")\n   278â†’    mosflm_probe_output.append(f\"TRACE_PY: real_b_reconstructed {b_real[0]:.15g} {b_real[1]:.15g} {b_real[2]:.15g}\")\n   279â†’    mosflm_probe_output.append(f\"TRACE_PY: real_c_reconstructed {c_real[0]:.15g} {c_real[1]:.15g} {c_real[2]:.15g}\")\n   280â†’\n   281â†’    # Stage 4: Re-derived reciprocal vectors (from real-space via cross products)\n   282â†’    # Per CLAUDE.md Rule #13, these are recalculated after real vector reconstruction\n   283â†’    # Crystal stores these in a_star, b_star, c_star (same as post-misset, but potentially updated)\n   284â†’    # To demonstrate the re-derivation, we can compute them manually:\n   285â†’    # V_actual = a Â· (b Ã— c)\n   286â†’    def cross_product(v1, v2):\n   287â†’        return torch.tensor([\n   288â†’            v1[1] * v2[2] - v1[2] * v2[1],\n   289â†’            v1[2] * v2[0] - v1[0] * v2[2],\n   290â†’            v1[0] * v2[1] - v1[1] * v2[0]\n   291â†’        ], dtype=dt