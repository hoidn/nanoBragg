ta.device matches the requested device\n   276â†’        - crystal.hkl_data.dtype matches the requested dtype\n   277â†’        - Structure factor lookup succeeds without device mismatch errors\n   278â†’        \"\"\"\n   279â†’        import torch\n   280â†’        from nanobrag_torch.models.crystal import Crystal, CrystalConfig\n   281â†’        from nanobrag_torch.config import BeamConfig\n   282â†’        from nanobrag_torch.io.hkl import read_hkl_file\n   283â†’\n   284â†’        # Check if scaled.hkl exists (supervisor test case)\n   285â†’        hkl_file = Path('scaled.hkl')\n   286â†’        if not hkl_file.exists():\n   287â†’            pytest.skip(\"scaled.hkl not found (required for Phase L supervisor command)\")\n   288â†’\n   289â†’        # Parse device and dtype\n   290â†’        device = torch.device(device_str)\n   291â†’        dtype = torch.float32 if dtype_str == \"float32\" else torch.float64\n   292â†’\n   293â†’        # Load HKL data\n   294â†’        hkl_array, hkl_metadata = read_hkl_file(str(hkl_file))\n   295â†’\n   296â†’        # Create minimal config (matching supervisor command)\n   297â†’        wavelength_A = 0.976800\n   298â†’        beam_config = BeamConfig(wavelength_A=wavelength_A)\n   299â†’\n   300â†’        config = CrystalConfig(\n   301â†’            cell_a=100.0,\n   302â†’            cell_b=100.0,\n   303â†’            cell_c=100.0,\n   304â†’            cell_alpha=90.0,\n   305â†’            cell_beta=90.0,\n   306â†’            cell_gamma=90.0,\n   307â†’            N_cells=(36, 47, 29),\n   308â†’        )\n   309â†’\n   310â†’        # Instantiate Crystal\n   311â†’        crystal = Crystal(config, beam_config=beam_config, device=device, dtype=dtype)\n   312â†’\n   313â†’        # Attach HKL data (mimicking __main__.py flow)\n   314â†’        if isinstance(hkl_array, torch.Tensor):\n   315â†’            crystal.hkl_data = hkl_array.clone().detach().to(device=device, dtype=dtype)\n   316â†’        else:\n   317â†’ 