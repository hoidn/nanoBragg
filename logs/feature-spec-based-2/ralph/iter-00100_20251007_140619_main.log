nce', '100',\n   167â†’            '-detpixels', '512',\n   168â†’            '-pixel', '0.1',\n   169â†’            '-oversample', '1',\n   170â†’            '-phisteps', '1',\n   171â†’            '-mosaic_dom', '1'\n   172â†’        ]\n   173â†’\n   174â†’        with tempfile.TemporaryDirectory() as tmpdir:\n   175â†’            tmpdir = Path(tmpdir)\n   176â†’            c_out = tmpdir / 'c_image.bin'\n   177â†’            py_out = tmpdir / 'py_image.bin'\n   178â†’\n   179â†’            # Run C (in tmpdir to avoid Fdump.bin contamination)\n   180â†’            # Convert c_bin to absolute path before changing directory\n   181â†’            c_bin_abs = str(Path(c_bin).resolve())\n   182â†’            c_cmd = [c_bin_abs] + common_args + ['-floatfile', str(c_out)]\n   183â†’            result_c = subprocess.run(c_cmd, capture_output=True, text=True, cwd=tmpdir)\n   184â†’            if result_c.returncode != 0:\n   185â†’                pytest.fail(f\"C simulation failed:\\n{result_c.stderr}\")\n   186â†’\n   187â†’            # Run PyTorch (in tmpdir to avoid Fdump.bin contamination)\n   188â†’            py_cmd = py_cli.split() + common_args + ['-floatfile', str(py_out)]\n   189â†’            result_py = subprocess.run(py_cmd, capture_output=True, text=True, cwd=tmpdir)\n   190â†’            if result_py.returncode != 0:\n   191â†’                pytest.fail(f\"PyTorch simulation failed:\\n{result_py.stderr}\")\n   192â†’\n   193â†’            # Load images\n   194â†’            shape = (512, 512)\n   195â†’            c_img = read_float_image(c_out, shape)\n   196â†’            py_img = read_float_image(py_out, shape)\n   197â†’\n   198â†’            # Compute metrics\n   199â†’            c_sum = np.sum(c_img)\n   200â†’            py_sum = np.sum(py_img)\n   201â†’            sum_ratio = py_sum / c_sum if c_sum > 0 else float('inf')\n   202â†’\n   203â†’            c_max = np.max(c_img)\n   204â†’            py_max = np.max(py_img)\n   205â†’            max_