rser.add_argument('-cell', nargs=6, type=float,\n    84â†’                        metavar=('a', 'b', 'c', 'Î±', 'Î²', 'Î³'),\n    85â†’                        help='Direct cell in Ã… and degrees')\n    86â†’    parser.add_argument('-img', type=str, metavar='FILE',\n    87â†’                        help='Read SMV header to set geometry')\n    88â†’    parser.add_argument('-mask', type=str, metavar='FILE',\n    89â†’                        help='Read SMV mask (0 values are skipped)')\n    90â†’    parser.add_argument('-sourcefile', type=str, metavar='FILE',\n    91â†’                        help='Multi-column text file with sources')\n    92â†’\n    93â†’    # Auxiliary S(Q) files (read but not used in this version per spec)\n    94â†’    parser.add_argument('-stol', type=str, metavar='FILE',\n    95â†’                        help='Structure factor vs sin(Î¸)/Î» for amorphous materials (read but not used)')\n    96â†’    parser.add_argument('-4stol', dest='stol', type=str, metavar='FILE',\n    97â†’                        help='Alias for -stol (read but not used)')\n    98â†’    parser.add_argument('-Q', dest='stol', type=str, metavar='FILE',\n    99â†’                        help='Alias for -stol (read but not used)')\n   100â†’    parser.add_argument('-stolout', type=str, metavar='FILE',\n   101â†’                        help='Output file for S(Q) (read but not used)')\n   102â†’\n   103â†’    # Structure factors\n   104â†’    parser.add_argument('-default_F', type=float, default=0.0,\n   105â†’                        help='Default structure factor (default: 0)')\n   106â†’\n   107â†’    # Detector geometry\n   108â†’    parser.add_argument('-pixel', type=float, metavar='MM',\n   109â†’                        help='Pixel size in mm (default: 0.1)')\n   110â†’    parser.add_argument('-detpixels', type=int, metavar='N',\n   111â†’                        help='Square detector pixel count')\n   112â†’    parser.add_argument('-detpixels_f', '-detpixels_x', type=int, meta