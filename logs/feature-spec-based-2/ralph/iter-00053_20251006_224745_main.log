ry image.\"\"\"\n    48â†’    data = np.fromfile(path, dtype=np.float32)\n    49â†’    if data.size != shape[0] * shape[1]:\n    50â†’        raise ValueError(f\"Expected {shape[0]}Ã—{shape[1]}={shape[0]*shape[1]} floats, got {data.size}\")\n    51â†’    return data.reshape(shape)\n    52â†’\n    53â†’\n    54â†’@pytest.mark.skipif(not is_parallel_enabled(), reason=\"NB_RUN_PARALLEL=1 required for Câ†”PyTorch parity\")\n    55â†’class TestFlattSquareMatchesC:\n    56â†’    \"\"\"Test that SQUARE lattice factor matches C implementation.\"\"\"\n    57â†’\n    58â†’    def test_f_latt_square_matches_c(self):\n    59â†’        \"\"\"\n    60â†’        Verify F_latt and I_before_scaling match between C and PyTorch for SQUARE shape.\n    61â†’\n    62â†’        This test runs a minimal SQUARE crystal simulation and compares final intensities.\n    63â†’        The ratio should be within 1e-3 (Phase K3 exit criterion).\n    64â†’\n    65â†’        CRITICAL FIX: This test validates that F_latt uses sincg(Ï€Â·h, Na) instead of\n    66â†’        the incorrect sincg(Ï€Â·(h-h0), Na) which caused a 463Ã— error (Attempt #28).\n    67â†’        \"\"\"\n    68â†’        if not is_parallel_enabled():\n    69â†’            pytest.skip(\"NB_RUN_PARALLEL=1 required\")\n    70â†’\n    71â†’        c_bin = get_c_binary()\n    72â†’        py_cli = f\"{sys.executable} -m nanobrag_torch\"\n    73â†’\n    74â†’        # Shared parameters (simple cubic, SQUARE shape)\n    75â†’        common_args = [\n    76â†’            '-cell', '100', '100', '100', '90', '90', '90',\n    77â†’            '-default_F', '300',\n    78â†’            '-N', '10',\n    79â†’            '-lambda', '1.0',\n    80â†’            '-distance', '100',\n    81â†’            '-detpixels', '512',\n    82â†’            '-pixel', '0.1',\n    83â†’            '-oversample', '1',\n    84â†’            '-phisteps', '1',\n    85â†’            '-mosaic_dom', '1'\n    86â†’        ]\n    87â†’\n    88â†’        with tempfile.Tem